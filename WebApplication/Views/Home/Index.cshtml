@model IEnumerable<Employee>;
@{
    // move this line to _ViewStart
    // Layout = "~/Views/Shared/_layout.cshtml";

    // u can say
    // layout = "_layout2" to override  or "null"
};
<table>
    <tr>
        <th>id</th>
        <th>name</th>
    </tr>

    <tbody>
        @foreach (var Employee in Model)
        {
            <tr>
                <th>@Employee.Id</th>
                <th>@Employee.Name</th>
            </tr>
        }
    </tbody>
</table>
@section Scripts{
    <script src="~/js/CustamScript.js"></script>
}